{
  "name": "svelte2dts",
  "description": "Generate d.ts files from svelte files",
  "version": "0.2.9",
  "author": "firefish5000 @firefish5000",
  "bin": {
    "svelte2dts": "./bin/svelte2dts"
  },
  "bugs": "https://github.com/firefish5000/svelte2dts/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "tslib": "^2.0.3",
    "itiriri": "^2.0.1",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.23.1",
    "@oclif/test": "^1.2.7",
    "@tdi/eslint-config-base": "^0.1.0",
    "@types/chai": "^4.2.14",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-formatter-vscode": "^0.1.6",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "svelte": "^3.28.0",
    "svelte2tsx": "^0.1.143",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "typescript": "^4.0",
    "svelte2tsx": "^0.1.143",
    "svelte": "^3.28.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": false
    },
    "svelte2tsx": {
      "optional": false
    },
    "svelte": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "bin/**/*",
    "out/**/*"
  ],
  "homepage": "https://github.com/firefish5000/svelte2dts",
  "keywords": [
    "svelte",
    "typescript",
    "preprocess",
    "ts",
    "dts",
    "d.ts",
    ".d.ts",
    "svelte.d.ts",
    ".svelte.d.ts",
    "cli"
  ],
  "license": "MIT",
  "main": "out/index.js",
  "oclif": {
    "bin": "svelte2dts"
  },
  "repository": "firefish5000/svelte2dts",
  "scripts": {
    "lint": "eslint --fix --format vscode ./src ./test",
    "clean": "git clean -fX ./out ./_build_",
    "prepublishOnly": "run-s clean build",
    "build": "run-p build:ts build:docs",
    "build:ts": "tsc -b",
    "build:docs": "oclif-dev readme",
    "prepare": "run-p build",
    "test": "run-s test:ci lint",
    "test:ci": "cross-env NODE_ENV=test jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "out/index.d.ts"
}
